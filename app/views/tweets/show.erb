<h1> Tweets by <%= @user.username %>:</h1>

<%= @tweet.content %> <BR><BR>


      <form method='update' action="/tweets/<%=@tweet.id%>/edit">
        <input type="submit" value="Edit Tweet"> </form>

    <form method="post" action="/tweets/<%=@tweet.id%>/delete"> <BR>
      <input type="hidden" name="_method" value="delete" id="hidden">
      <input data-confirm='Are you sure?' type="submit" value="Delete Tweet" > </form>
<a href = '/tweets'> Return to All My Tweets </a>
